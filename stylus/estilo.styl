@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?-h067f0');
	src:url('../fonts/icomoon.eot?#iefix-h067f0') format('embedded-opentype'),
		url('../fonts/icomoon.woff?-h067f0') format('woff'),
		url('../fonts/icomoon.ttf?-h067f0') format('truetype'),
		url('../fonts/icomoon.svg?-h067f0#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-Celsius:before {
	content: "\e600";
}

.icon-Fahrenheit:before {
	content: "\e601";
}

.icon-three-bars:before {
	content: "\f05e";
}

.icon-search:before {
	content: "\e986";
}

gris_azulado = #353C45
gris_claro = #788594
verde = #95BF3D

body
	font-size 16px
	background url("../img/PRINCIPAL -01.jpg") no-repeat
	background-size 100%

.cuerpo
	margin 1em 6em 0em 6em

.slider
	background url('../img/18.jpg')
	background-size 100% 100%
	height 20.1em

.cuerpoAccesorios
	margin 1em 0em
	width 100%
	position relative
	overflow hidden

.accesoriosIzq
	float left
	width: 68%

.menu
	width 100%
	background gris_azulado
	text-align center
	position relative
	border-radius 5px
	margin-top 1em

.menuItem
	margin 0
	padding 0
	list-style none
	.item
		display inline-block
		a
			color black
			text-decoration none
			display block
			padding: 0.7em 2em;
			-webkit-transition: .2s;
			-moz-transition: .2s;
			color gris_claro
			&:hover
				background verde
				color white
				-webkit-transition: .2s;
				-moz-transition: .2s;

.accesorios
	list-style none
	width 50%
	display inline-block

.buscador
	padding 0.4em 1em
	border-left 1px solid gris_claro
	border-top 1px solid gris_claro
	border-bottom 1px solid gris_claro
	border-right 0px
	border-radius 5px 0px 0px 5px

.botonBus
	background verde
	padding 0.46em 1em
	position absolute
	border-radius 0px 5px 5px 0px
	color white
	cursor pointer
	border-left 0px
	border-top 1px solid gris_claro
	border-bottom 1px solid gris_claro
	border-right 1px solid gris_claro
	border 1px solid gris_claro
	
.accesorio
	width 100%
	display block
	margin 1em 0em 1em 0em

.calendary
	width 84%
	height 19.8em

.twitter
	width 49%
	display inline-block
	max-height 19.8em

.twitter-barra
	width 100%
	background gris_azulado
	border-radius 5px 5px 0px 0px
	a
		color white
		font-size 3em
		text-decoration none
		background verde
		border-radius 5px 0px 0px 0px
.accesorioDrechos
	float right
	width 30%

.listaDerecha
	list-style none
	height 20%
	.itemD
		margin-bottom 1em
		figure
			height 150px
			img
				width 100%
				height 100%
				border-radius 5px 5px 0px 0px
				padding 0
				margin 0
		p
			color gris_claro
			text-align center
			background gris_azulado
			margin 0
			padding 0
			border-radius 0px 0px 5px 5px
		.clima
			padding 0
			margin 0
			width 0
			text-align center
	.climaD
		padding-left 2em

footer
	margin 0em 6em 2em 6em
.listaBaja
	margin 0
	list-style none

.noticiasB
	width 30%
	display inline-block

.noticiaFigure
	width 100%
	border-radius 5px 5px 0px 0px
	background gris_azulado

.imgNf
	width 100%
	border-radius 5px 5px 0px 0px

.pNf
	width 100%
	color white
	background gris_azulado
	border-radius 0px 0px 5px 5px
	padding 0em 1em

.videoB
	display inline-block
	width 68%

.videoYT
	width 70%
	height 15.9em
	border-radius 5px
	background gris_azulado
	padding .5em

.face
	display inline-block
	position absolute
	height 37%
	padding .2em .2em .2em 0em
	margin 0em 0em 0em .3em
	background #f6f7f8
	
